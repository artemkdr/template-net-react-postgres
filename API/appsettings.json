{
  "SwaggerUrlPrefix": "/api/",   
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Consul": {
    "ServiceID": "api",
    "Address": "http://consul:8500",
    "HealthCheck": "health"
  },  
  "JwtSettings": {
    "Secret": "$JWT_SECRET_MUST_BE_LONGER_THAN_32_CHARS",
    "Issuer": "$JWT_ISSUER",
    "Audience": "$JWT_AUDIENCE",
    "ExpireInDays": 5
  },
  "ConnectionStrings": {
    "DefaultConnection": "$DB_CONNECTION_STRING"
  },
  "AllowedHosts": "*",  
  "AllowedOrigins": "http://localhost:3000,http://localhost" 
}
